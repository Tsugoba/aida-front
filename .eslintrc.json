{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "react-hooks/exhaustive-deps": "warn"
  },
  "ignorePatterns": ["src/**/*.spec.ts", "**/*.spec.ts", "**/*.spec.tsx"],
  "parserOptions": {
    "project": "./tsconfig.lint.json"
  }
}
